{
  "name": "sakurascript",
  "version": "1.1.2",
  "description": "SakuraScript Parser/Builder",
  "keywords": [
    "ikagaka",
    "ukagaka",
    "sakurascript"
  ],
  "main": "./dist/lib/sakurascript.js",
  "types": "./dist/lib/sakurascript.d.ts",
  "files": [
    "dist",
    "web"
  ],
  "scripts": {
    "prepare": "npm run dist",
    "prepublishOnly": "npm run lint && npm run test",
    "dist": "shx rm -rf dist && cross-env NODE_ENV=production webpack",
    "lint": "npm run eslint && npm run tslint",
    "eslint": "eslint *.js",
    "tslint": "tslint -p .",
    "test": "npm run test-node && npm run test-web",
    "test-cli": "npm run test-node && npm run test-web-headless",
    "test-node": "cross-env NODE_ENV=test nyc mocha",
    "test-web": "cross-env NODE_ENV=test karma start --single-run",
    "test-web-headless": "cross-env NODE_ENV=test NO_DETECT=1 karma start --single-run --browsers ChromeHeadless",
    "docdist": "cd doc && git clean -fd && git rm -rf * && cd .. && npm run doc && cd doc && touch .nojekyll && git add . && git commit -m doc && git push",
    "doc": "typedoc --mode file --out doc --readme Readme.md --disableOutputCheck --excludeNotExported lib && cd doc && git status && cd .."
  },
  "dependencies": {},
  "devDependencies": {
    "@types/mocha": "^5.0.0",
    "@types/node": "^10.0.3",
    "@types/power-assert": "^1.4.29",
    "cross-env": "^5.1.3",
    "eslint": "^4.15.0",
    "eslint-config-narazaka": "^1.0.1",
    "espower-typescript": "^8.1.2",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-detect-browsers": "^2.2.6",
    "karma-edge-launcher": "^0.4.2",
    "karma-espower-preprocessor": "^1.1.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-opera-launcher": "^1.0.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "mocha": "^5.0.0",
    "nyc": "^12.0.1",
    "power-assert": "^1.4.4",
    "shx": "^0.2.2",
    "ts-loader": "^3.2.0",
    "tslint": "^5.9.1",
    "typedoc": "^0.11.0",
    "typescript": "^2.7.2",
    "webpack": "^3.10.0"
  },
  "url": "https://github.com/Ikagaka/sakurascript",
  "author": "Ikagaka",
  "license": "MIT",
  "contributors": [
    "Narazaka",
    "legokichi"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Ikagaka/sakurascript.git"
  }
}
